{
  "name": "frontend-monorepo-boilerplate",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "turbo build",
    "dev": "turbo dev --parallel --no-cache",
    "lint": "turbo lint",
    "lint-staged-husky": "lint-staged",
    "tsc": "turbo tsc",
    "test": "turbo test",
    "test-coverage": "turbo test --coverage",
    "storybook": "turbo storybook",
    "storybook-build": "turbo storybook-build",
    "format-lint": "prettier --config .prettierrc --check --ignore-unknown .",
    "format-fix": "prettier --config .prettierrc --write --ignore-unknown -l .",
    "commit": "git-cz",
    "clean": "npm exec --workspaces -- shx rm -rf node_modules .next .turbo coverage dist compiled build-next-static build-storybook-static .tmp .yarn.lock yarn-error.log && shx rm -rf node_modules",
    "remove-turbo-cache": "shx rm -rf ./node_modules/.cache/turbo",
    "update-dependencies": "turbo update-dependencies && npx npm-check-updates -u"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@hookform/resolvers": "3.3.2",
    "@mui/icons-material": "5.14.16",
    "@mui/material": "5.14.16",
    "@mui/x-date-pickers": "6.17.0",
    "@tanstack/react-query": "5.4.3",
    "@tanstack/react-query-devtools": "5.4.3",
    "axios": "1.6.0",
    "http-status": "1.7.3",
    "i18next": "23.6.0",
    "i18next-browser-languagedetector": "7.1.0",
    "immer": "10.0.3",
    "jsonwebtoken": "9.0.2",
    "luxon": "3.4.3",
    "next": "14.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.47.0",
    "react-i18next": "13.3.1",
    "react-intersection-observer": "9.5.2",
    "zod": "3.22.4",
    "zustand": "4.4.5"
  },
  "devDependencies": {
    "@anatine/zod-mock": "3.13.3",
    "@commitlint/cli": "18.2.0",
    "@commitlint/config-conventional": "18.1.0",
    "@faker-js/faker": "8.2.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-actions": "7.5.2",
    "@storybook/addon-essentials": "7.5.2",
    "@storybook/addon-interactions": "7.5.2",
    "@storybook/addon-links": "7.5.2",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/builder-webpack5": "7.5.2",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/react": "7.5.2",
    "@storybook/theming": "7.5.2",
    "@testing-library/dom": "9.3.3",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.1",
    "@types/jsonwebtoken": "9.0.4",
    "@types/luxon": "3.3.3",
    "@types/node": "20.8.10",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "commitizen": "4.3.0",
    "husky": "8.0.3",
    "lint-staged": "15.0.2",
    "msw": "2.0.2",
    "prettier": "3.0.3",
    "pretty-quick": "3.1.3",
    "semantic-release": "22.0.6",
    "semantic-release-gitmoji": "1.6.5",
    "shx": "0.3.4",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tsup": "7.2.0",
    "turbo": "1.10.16",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": ">=18.15.0",
    "npm": ">=9.5.0",
    "yarn": ">=1.22.19"
  }
}
